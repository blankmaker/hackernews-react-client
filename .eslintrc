{
    "rules": {
        "indent": [2, 2],
        "quotes": [2, "single"],
        "linebreak-style": [2, "unix"],
        "semi": [2, "always"],
        "no-console": 1,
        "no-trailing-spaces": 2,
        "no-irregular-whitespace": 1,
        "comma-dangle": 1,
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "experimentalObjectRestSpread": true
    },
    "plugins": [
        "react"
    ]
}